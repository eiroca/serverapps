<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN" "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
  <display-name>Assembler Proxy</display-name>
  <description>Assembler Proxy</description>
  <distributable />
  <servlet>
    <servlet-name>Assembler</servlet-name>
    <display-name>Assembler</display-name>
    <description>Assembler Servlet</description>
    <servlet-class>net.eiroca.portal.assembler.AssemblerServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>Assembler</servlet-name>
    <url-pattern>/run/*</url-pattern>
  </servlet-mapping>
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
